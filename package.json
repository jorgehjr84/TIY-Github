{
  "name": "TIY-Github",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "browser-sync start --server --startPath src/",
    "start": "browser-sync start --server --files='index.html, css/*.css, js/*.js' --startPath src/index.html",
    "test": "browser-sync start --server --files='js/*.js,test/index.html,test/spec/**/*.js' --startPath test/index.html",
    "postinstall": "npm run bower:install",
    "bower:install": "bower install --save",
    "postbower:install": "wiredep --src src/index.html",
    "sass": "node-sass src/scss/main.scss -o src/css"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "wiredep": "^2.2.2"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/jorgehjr84/TIY-Github.git"
  },
  "author": "Jorge Hernandez",
  "license": "CC0",
  "bugs": {
    "url": "https://github.com/jorgehjr84/TIY-Github/issues"
  },
  "homepage": "https://github.com/jorgehjr84/TIY-Github"
}